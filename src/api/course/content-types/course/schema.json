{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "seccions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::seccion.seccion",
      "mappedBy": "course"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "teacher": {
      "type": "string"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "video": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "price": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "studentsqty": {
      "type": "string"
    },
    "duration": {
      "type": "integer"
    },
    "level": {
      "type": "enumeration",
      "enum": [
        "BASIC",
        "MIDDLE",
        "PRO"
      ]
    },
    "instructor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "isfree": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "mode": {
      "type": "enumeration",
      "enum": [
        "VIVO",
        "RECORD"
      ],
      "default": "RECORD",
      "required": true
    },
    "datevivo": {
      "type": "datetime"
    },
    "preview_video": {
      "type": "string"
    },
    "reduced_price": {
      "type": "decimal"
    },
    "session": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::session.session",
      "inversedBy": "courses"
    }
  }
}
